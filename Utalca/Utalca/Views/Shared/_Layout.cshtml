<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</head>
<body>
    <div class="top-header bg-white hidden-xs">
        <div class="container">
            <div class="row">
                <div class="col-sm-6">
                    &nbsp;
                </div>
                <div class="col-sm-6">
                    <ul class="list-inline pull-right">
                        <li><a target="_blank" href="https://es-es.facebook.com/ingenieriaUtalca" class="icon-holder small rounded"><i class="fa fa-facebook"></i></a></li>
                        <li><a target="_blank" href="https://twitter.com/ingUtalca" class="icon-holder small rounded"><i class="fa fa-twitter"></i></a></li>
                        <li><a target="_blank" href="https://cl.linkedin.com/in/utalca" class="icon-holder small rounded"><i class="fa fa-linkedin"></i></a></li>
                        <li><a target="_blank" href="https://www.youtube.com/user/canalUtalca" class="icon-holder small rounded"><i class="fa fa-youtube-play"></i></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    @Html.Partial("_MenuProfesor")
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>
    @Html.Partial("_Errores")

    <script>
        $(document).ready(function () {
            if ($('#ErroresModal').length > 0)
            {
                $("#ErroresModal").modal('show')
            }
            
        });
    </script>
</body>
</html>